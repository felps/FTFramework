<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>
    <class name="org.gridlab.gridsphere.services.core.security.auth.modules.impl.JaasCallbackHandler" table="handlers">
        <id name="username" column="husername" type="java.lang.String" length="32"/>
        <property name="password" type="string" column="pass"/>
    </class>
</hibernate-mapping>
